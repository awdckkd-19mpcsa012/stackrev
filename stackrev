import java.util.*;
class stackrev
{
public static void main(String[]args)
{
Scanner sc=new Scanner(System.in);
Stack s=new Stack();
String str=sc.next();
char ch[]=str.toCharArray();
for(int i=0;i<ch.length;i++)
{
s.push(ch[i]);
}
for(int i=0;i<ch.length;i++)
{
System.out.print(s.pop());
}}}
------------------------------------------
OUTPUT:
D:\>java stackrev
ADITYA
AYTIDA
